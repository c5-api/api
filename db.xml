<?xml version="1.0"?>
<schema version="0.3">
  <table name="apiRouteRegistry">
    <field name="id" type="I" size="10">
      <KEY/>
      <AUTOINCREMENT/>
      <UNSIGNED/>
    </field>
    <field name="pkgId" type="I" size="10">
      <NOTNULL/>
    </field>
    <field name="pkgHandle" type="C" size="255">
      <NOTNULL/>
    </field>    
    <field name="routeName" type="C" size="255">
      <NOTNULL/>
    </field>
    <field name="enabled" type="I1" size="1">
      <NOTNULL/>
      <DEFAULT value="1"/>      
    </field>
    <field name="rateLimit" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>
    <field name="rateCount" type="I" size="10">
      <NOTNULL/>
      <DEFAULT value="0"/>
    </field>    
  </table>
</schema>